{
    "name": "tigusigalpa/coingecko-php",
    "description": "A comprehensive PHP/Laravel package for CoinGecko API integration with support for all endpoints including coins, markets, contracts, and more. Easy to use, well-documented, and Laravel-ready.",
    "type": "library",
    "keywords": [
        "coingecko",
        "cryptocurrency",
        "crypto",
        "api",
        "laravel",
        "php",
        "bitcoin",
        "ethereum",
        "blockchain",
        "market-data",
        "crypto-prices"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Sazonov",
            "email": "sovletig@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tigusigalpa\\Coingecko\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tigusigalpa\\Coingecko\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tigusigalpa\\Coingecko\\CoingeckoServiceProvider"
            ],
            "aliases": {
                "Coingecko": "Tigusigalpa\\Coingecko\\Facades\\Coingecko"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
